{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@global/*": [
        "src/global/*"
      ],
      "@mixins/*": [
        "src/mixins/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@entities/*": [
        "src/entities/*"
      ],
      "@vars/*": [
        "src/vars/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    }
  }
}